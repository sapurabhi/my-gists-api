# Ignore common Java build artifacts
target/
*.class
*.jar
*.war
*.ear
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
release.properties
dependency-reduced-pom.xml

# Ignore Maven wrapper files that are downloaded locally
.mvn/wrapper/maven-wrapper.jar
.mvn/wrapper/maven-wrapper.properties

# Ignore IDE-specific files and directories
.idea/                 # IntelliJ IDEA
.vscode/               # VS Code
*.iml                  # IntelliJ IDEA module files
*.ipr                  # IntelliJ IDEA project files
*.iws                  # IntelliJ IDEA workspace files
.project               # Eclipse
.classpath             # Eclipse
.settings/             # Eclipse

# Ignore Git-related files
.git/                  # The Git repository itself
.gitignore             # This file is for Git, not needed in Docker image

# Ignore logs and temporary files
*.log
logs/
tmp/
temp/

# Ignore OS-generated files
.DS_Store              # macOS
Thumbs.db              # Windows

# Ignore environment configuration files
# IMPORTANT: Never commit actual secrets, but if you have .env files for local dev, exclude them.
.env
.env.*